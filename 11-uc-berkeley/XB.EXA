MARK NEXT
LINK 800
LINK 800
HOST T
TEST X = T
FJMP NEXT

;LOAD TAPE
COPY M X; ENTRY
GRAB 200
SEEK 9999; FROM THE END
SEEK -3 ;LAST ENTRY NAME
MARK SEEK
TEST F = X
SEEK -4 ;PREV ENTRY NAME
FJMP SEEK
SEEK 4 ;MATCH

COPY F X; START POSITION
COPY F T; CHARS COUNT
SEEK -9999
SEEK X ;GOTO START

MARK EXTRACT
SUBI T 1 T ;LOWER TO 0
COPY F M
TJMP EXTRACT
COPY 0 M ;0 = END