LINK 800

COPY M T ;EAST
FJMP NEXT ;IF =0

MARK EAST
LINK 801
SUBI T 1 T
TJMP EAST

MARK NEXT
COPY M X ;WHERE TO GO
COPY M T ;HOW MANY TIMES
FJMP EDIT ;IF =0

MARK NORTH_SOUTH
LINK X
SUBI T 1 T
TJMP NORTH_SOUTH

MARK EDIT
GRAB 200
COPY M X

MARK SEEKER
TEST X = F
SEEK 5 ;NEXT NAME
FJMP SEEKER
SEEK -5 ;BACK TO STARS

COPY F X ;COPY A STAR

@REP 4
