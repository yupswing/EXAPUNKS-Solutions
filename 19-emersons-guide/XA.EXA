GRAB 300

SEEK 1 ;SKIP NAME
COPY F M ;SEND EAST

COPY F X ;NORTH/SOUTH
TEST X < 0
TJMP SOUTH

;MARK NORTH ;OR NOTHING
COPY 800 M ;WHERE (N)
COPY X M ;HOW MANY TIMES
JUMP END

MARK SOUTH
COPY 802 M ;WHERE (S)
MULI X -1 M ;HOW MANY TI

MARK END
SEEK -3 ;BACK TO NAME
COPY F M ;SEND NAME